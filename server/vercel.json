{
  "version": 2,
    "builds": [
      {
        "src": "./dist/main.js",
        "use": "@vercel/node"
      }
    ],
  "routes": [
    {
      "src": "./dist/*",
      "dest": "./dist/main.js",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "PATCH",
        "DELETE"
      ]
    }
  ]
}